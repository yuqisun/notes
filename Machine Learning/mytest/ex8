1.estimateGaussian.m

mu=sum(X)./m;
sigma2=sum((X.-mu).^2)./m;


2.selectThreshold.m

	tp=sum((pval<epsilon)&(yval==1));
	fp=sum((pval<epsilon)&(yval==0));
	fn=sum((pval>=epsilon)&(yval==1));

	prec=tp/(tp+fp);
	rec=tp/(tp+fn);

	F1=2*prec*rec/(prec+rec);
  
  
  
3.cofiCostFunc.m

J=sum(((X*Theta' - Y).^2)(R==1))/2;

